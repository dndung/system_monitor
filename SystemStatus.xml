<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="vp9.system_status">
    <signal name="networkChanged">
      <arg name="networkStatus" type="i" direction="out"/>
    </signal>
    <signal name="hddChanged">
      <arg name="hddStatus" type="i" direction="out"/>
    </signal>
    <method name="getNetworkStatus">
      <arg type="i" direction="out"/>
    </method>
    <method name="getHddStatus">
      <arg type="i" direction="out"/>
    </method>
  </interface>
</node>
